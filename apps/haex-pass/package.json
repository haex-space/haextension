{
  "name": "haex-pass",
  "version": "1.4.33",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build --preset static",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "package": "haex sign .output/public",
    "build:release": "pnpm build && pnpm package",
    "build:patch": "node scripts/bump-version.js patch && pnpm build:release",
    "build:minor": "node scripts/bump-version.js minor && pnpm build:release",
    "build:major": "node scripts/bump-version.js major && pnpm build:release",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "ext:dev": "haex dev",
    "ext:build": "nuxt build --preset static && haex sign .output/public",
    "db:generate": "drizzle-kit generate && npx tsx scripts/fix-migrations.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@haex-space/vault-sdk": "^2.5.87",
    "@iconify/vue": "^5.0.0",
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/icon": "^2.1.1",
    "@nuxtjs/i18n": "^10.2.1",
    "@pinia/nuxt": "^0.11.3",
    "@vueuse/core": "^14.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "embla-carousel-vue": "^8.6.0",
    "fuse.js": "^7.1.0",
    "hash-wasm": "^4.12.0",
    "kdbxweb": "^2.1.1",
    "lucide-vue-next": "^0.562.0",
    "nuxt": "^4.2.2",
    "otpauth": "^9.4.1",
    "photoswipe": "^5.4.4",
    "reka-ui": "^2.6.1",
    "shadcn-nuxt": "2.3.2",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "vaul-vue": "^0.4.1",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@nuxtjs/color-mode": "^4.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "tailwindcss": "^4.1.18",
    "vitest": "^3.2.3"
  },
  "pnpm": {
    "overrides": {
      "drizzle-orm": "0.45.1"
    }
  }
}
