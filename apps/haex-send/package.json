    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "upload": "node scripts/bump-version.js patch --upload",
    "upload:patch": "node scripts/bump-version.js patch --upload",
    "upload:minor": "node scripts/bump-version.js minor --upload",
    "upload:major": "node scripts/bump-version.js major --upload",
